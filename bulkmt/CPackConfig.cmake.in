# Hand written CPack configuration

SET(CPACK_DEBIAN_PACKAGE_CONTACT "rastopyakin@gmail.com")
SET(CPACK_DEBIAN_PACKAGE_MAINTAINER "Alexander Popov <rastopyakin@gmail.com>")

SET(CPACK_CMAKE_GENERATOR "Unix Makefiles")
SET(CPACK_GENERATOR "DEB")
SET(CPACK_INSTALL_CMAKE_PROJECTS "@CMAKE_CURRENT_BINARY_DIR@;@PROJECT_NAME@;ALL;/")

SET(CPACK_PACKAGE_DEFAULT_LOCATION "/")
SET(CPACK_PACKAGE_NAME "@PROJECT_NAME@")
SET(CPACK_PACKAGE_FILE_NAME "@PROJECT_NAME@-@PROJECT_VERSION@")
SET(CPACK_PACKAGE_DESCRIPTION_FILE "@PROJECT_SOURCE_DIR@/README")
SET(CPACK_PACKAGE_DESCRIPTION_SUMMARY "bulkmt task (10)")
SET(CPACK_PACKAGE_VERSION "@PROJECT_VERSION@")
